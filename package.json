    "postcss-loader": "^7.3.3",
    "redux-mock-store": "^1.5.4",
    "source-map-loader": "^4.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
